citHeader("To cite the 'rmdcev' package in publications use:")

year = 2020
vers = paste('R package version', meta$Version)

citEntry(
  entry = 'manual',
  title = paste('rmdcev:', meta$Title),
  author = Filter(function(p) 'aut' %in% p$role, as.person(meta$Author)),
  year = year,
  note = vers,
  url = meta$URL,
  textVersion = paste('Patrick Lloyd-Smith (', year, '). rmdcev: ', meta$Title, '. ', vers, '.', sep = '')
)
